fun sum(a, b) {
    return a + b
}

var result = sum(10, 2)
io.println(result) // expect: 12

fun main(args: List<string>): void {
    io.println(args) // expect: ["Hello", "World"]
}

main(["Hello", "World"])

fun do_something(cb) {
    cb("Hello") // expect: Hello
}

do_something(io.println)

fun outside(x: f64): any {
    fun inner(): f64 {
        return x
    }

    return inner
}

var closure = outside(10)

io.println(closure()) // expect: 10