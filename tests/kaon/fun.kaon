fun sum(a, b) {
    return a + b
}

var result = sum(10, 2)
print(result) // expect: 12

fun main(args: List<string>): void {
    print(args) // expect: ["Hello", "World"]
}

main(["Hello", "World"])

fun do_something(cb) {
    cb("Hello") // expect: Hello
}

do_something(print)

fun outside(x: f64): any {
    fun inner(): f64 {
        return x
    }

    return inner
}

var closure = outside(10)

print(closure()) // expect: 10

fun one() {
    fun two() {
        fun three() {
            print("Sup.")
        }

        three()
    }

    two()
}

one()