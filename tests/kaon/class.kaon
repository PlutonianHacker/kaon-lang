class T {}

print(T()) // expect: <instance T>

class Example {
    var x = 10

    fun update(new_x) {
        self.x = self.x + new_x

        print(self.x) // expect: 12
    }
}

var e = Example()
e.update(2)

class Vector {
    var x = 0
    var y = 0

    create new(a, b) {
        self.x = a
        self.y = b
    }

    fun get_x() {
        return self.x
    } 

    fun get_y() {
        return self.y
    }
}

var f = Vector.new(1, 2)

print(f.get_x()) // expect: 1
print(f.get_y()) // expect: 2

class Chain {
    var x = 1

    fun one() {
        self.x = self.x + 5
    }

    fun two() {
        self.x = self.x + 3
    }

    fun three() {
        print(self.x) // expect: 9
    }
}

var c = Chain()

c.one()
c.two()
c.three()