// number literal
print(123) // expect: 123

print(12.0) // expect: 12

print(12.3) // expect: 12.3

print(-123) // expect: -123

// string literal
print("Hello!") // expect: Hello!

// list literal
var list = [1, 2, 3]

print(list[0]) // expect: 1
print(list[2]) // expect: 3
print(list[-1]) // expect: 3
print(list[-3]) // expect: 1

var fruit = ["apple", "orange", "banana", "hamburger"]

print(fruit) // expect: ["apple", "orange", "banana", "hamburger"]

fruit[3] = "carrot"

print(fruit) // expect: ["apple", "orange", "banana", "carrot"]

fruit[-1] = "grapefruit"

print(fruit) // expect: ["apple", "orange", "banana", "grapefruit"]
